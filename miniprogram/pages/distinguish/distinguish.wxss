.distinguish_box{
  width: 100%;
  height: 100vh;
  position: relative;
  box-sizing: border-box;
  padding: 0 3%;
}
.content{
  width: 100%;
  font-size: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.conten_box{
  width: 49%;
  background-color: #fff;
  text-align: center;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  padding-top: 15rpx;
  margin-top: 10rpx;
  border-radius: 20rpx
}
.img_warp{
  width: 100%;
  height: 140rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content_title{
  font-size: 24rpx;
  color: #1a1b3c;
  padding: 30rpx 0;
}
.bottom{
  width: 100%;
  height: 10rpx;
  background-color: #fff;
  position: absolute;
  left: 0;
  bottom: 0;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
}